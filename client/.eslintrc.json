{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".tsx", ".jsx"] }],
    "import/prefer-default-export": "off",
    "@typescript-eslint/explicit-function-return-type": ["warn", {
      "allowExpressions": true
    }]
  }
} 